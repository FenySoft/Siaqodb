<Window x:Class="SiaqodbManager.SetTrialLicense"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="SetTrialLicense" Height="214" Width="441" WindowStartupLocation="CenterScreen" WindowStyle="ToolWindow" ResizeMode="NoResize">
    <Grid>
        <Label Content="Put your trial license key:" Height="28" HorizontalAlignment="Left" Margin="30,25,0,0" Name="label1" VerticalAlignment="Top" Width="141" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="40,59,0,0" Name="textBox1" VerticalAlignment="Top" Width="358" />
        <Button Content="OK" Height="23" HorizontalAlignment="Left" IsDefault="True" Margin="244,0,0,12" Name="btnOK" VerticalAlignment="Bottom" Width="75" Click="btnOK_Click" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Right" IsCancel="True" Margin="0,0,19,12" Name="btnCancel" VerticalAlignment="Bottom" Width="75" Click="btnCancel_Click" />

    </Grid>
</Window>
